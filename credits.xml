<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_credits"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="nonexistent.safeu_12.Credits"
    android:orientation="vertical"
    android:padding="25dp">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/Other"
        android:paddingBottom="15dp"
        android:paddingTop="10dp">

        <TextView
            android:text="Created and programmed by Molly Taylor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/Me"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ImageCredits"
        android:paddingBottom="15dp">

        <TextView
            android:text="Images"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Images"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textStyle="bold"
            android:paddingBottom="5dp" />

        <TextView
            android:text="emergency by Hea Poh Lin from the Noun Project"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/alertButton"
            android:paddingLeft="10dp"
            android:paddingBottom="7dp"
            android:layout_weight="1" />

        <TextView
            android:text="Check Mark by Kimmi Studio from the Noun Project"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/checkButton"
            android:paddingLeft="10dp"
            android:paddingBottom="7dp"
            android:layout_weight="1" />

        <TextView
            android:text="Email by Zaff Studio from the Noun Project"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/notificationsButton"
            android:paddingLeft="10dp"
            android:paddingBottom="7dp"
            android:layout_weight="1" />

        <TextView
            android:text="Settings by Aya Sofya from the Noun Project"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/settingsButton"
            android:paddingLeft="10dp"
            android:paddingBottom="7dp"
            android:layout_weight="1" />

        <TextView
            android:text="Power by i cons from the Noun Project"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/powerButton"
            android:paddingLeft="10dp"
            android:paddingBottom="7dp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/MentorsList"
        android:paddingBottom="15dp">

        <TextView
            android:text="Mentors"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Mentors"
            android:paddingBottom="5dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Doan"
            android:paddingLeft="10dp"
            android:text="Susan Doan"
            android:paddingBottom="7dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Kassa"
            android:paddingLeft="10dp"
            android:text="Jonathan Kassa"
            android:paddingBottom="7dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="7dp"
            android:paddingLeft="10dp"
            android:text="Bastiaan Van Eeckhoudt" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="7dp"
            android:paddingLeft="10dp"
            android:text="Jeremy Elson" />
    </LinearLayout>

</LinearLayout>
